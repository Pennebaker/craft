{
  "name": "pennebaker/craft",
  "description": "pennebaker Craft 3 CMS scaffolding project",
  "version": "1.0.13",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "digital@pennebaker.com",
    "issues": "https://github.com/pennebaker/craft/issues",
    "source": "https://github.com/pennebaker/craft",
    "docs": "https://github.com/pennebaker/craft"
  },
  "minimum-stability": "RC",
  "require": {
    "php": ">=7.0.0",
    "craftcms/cms": "^3.0.0-RC1",
    "vlucas/phpdotenv": "^2.4.0",
    "roave/security-advisories": "dev-master",
    "yiisoft/yii2-redis": "^2.0.6",
    "craftcms/guest-entries": "^2.1.3",
    "craftcms/mandrill": "^1.1.0",
    "nystudio107/craft-scripts": "^1.2.4",
    "nystudio107/craft3-cookies": "^1.1.7",
    "nystudio107/craft3-eagerbeaver": "^1.0.0",
    "nystudio107/craft3-fastcgicachebust": "^1.0.4",
    "nystudio107/craft3-imageoptimize": "^1.4.7",
    "nystudio107/craft3-instantanalytics": "^1.0.4",
    "nystudio107/craft3-minify": "^1.2.6",
    "nystudio107/craft3-multi-environment": "^1.0.5",
    "nystudio107/craft3-pathtools": "^1.0.6",
    "nystudio107/craft3-routemap": "^1.0.1",
    "nystudio107/craft3-richvariables": "^1.0.15",
    "nystudio107/craft3-transcoder": "^1.0.8",
    "nystudio107/craft3-typogrify": "^1.1.5",
    "aelvan/preparse-field": "v1.0.0",
    "charliedev/advanced-url-field": "^1.0.0",
    "charliedev/element-map": "^1.0.2",
    "dolphiq/iconpicker": "^1.0.4",
    "dolphiq/jsonclient": "v1.0.1",
    "dolphiq/sitemap": "v1.0.5",
    "doublesecretagency/craft-inventory": "^2.0.0",
    "enupal/backup": "^1.0.6",
    "marionnewlevant/match-input": "^2.0.0",
    "marionnewlevant/snitch": "v2.0.0",
    "markdrzy/importable": "v1.0.1",
    "markhuot/craftql": "^1.0.0-beta.26",
    "mmikkel/cp-field-inspect": "^1.0",
    "mmikkel/lettering": "^1.0.1",
    "mikestecker/craft3-readability": "^1.0.2",
    "newism/craft3-fields": "^0.0.7",
    "ostark/upper": "^1.1.1",
    "superbig/craft3-autologin": "^1.0.0",
    "superbig/craft3-http2serverpush": "^1.0.0",
    "verbb/cp-nav": "^2.0.2",
    "verbb/expanded-singles": "^1.0.1",
    "craftcms/element-api": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\sitemodule\\": "modules/sitemodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome",
      "@php pen-setup welcome"
    ],
    "post-update-cmd": [
      "@php pen-setup update"
    ],
    "post-install-cmd": [
      "@php nys-setup update"
    ]
  }
}
