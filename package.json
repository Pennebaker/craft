{
    "name": "nystudio107",
    "version": "1.0.0",
    "description": "Website for nystudio107.com",
    "main": "gulpfile.js",
    "author": "Andrew Welch, nystudio107 <andrew@nystudio107.com>",
    "copyright": "nystudio107",
    "license": "UNLICENSED",
    "private": true,
    "paths": {
        "src": {
            "base": "./src/",
            "fontello": "./src/fontello/",
            "fonts": "./src/fonts/",
            "img": "./src/img/",
            "json": "./src/json/",
            "js": "./src/js/",
            "scss": "./src/scss/"
        },
        "dist": {
            "base": "./web/",
            "css": "./web/css/",
            "js": "./web/js/",
            "fonts": "./web/fonts/",
            "img": "./web/img/",
            "img": "./web/json/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/css/",
            "fontello": "./build/fonts/fontello/",
            "fonts": "./build/fonts/",
            "js": "./build/js/",
            "html": "./build/html/",
            "img": "./build/img/",
            "json": ".build/json/"
        },
        "favicon": {
            "src": "./src/img/favicon_src.png",
            "dest": "./web/img/site/",
            "path": "/img/site/"
        },
        "scss": [
            "./node_modules",
            "./node_modules/bourbon/core",
            "./node_modules/sag-bourbon-neat/core",
            "./node_modules/normalize-sass",
            "./node_modules/choices.js/assets/styles/scss",
            "./node_modules/video.js/src/css",
            "./build/fonts/fontello/scss"
        ],
        "craftConfig": "./config/",
        "templates": "./templates/"
    },
    "urls": {
        "live": "https://nystudio107.com/",
        "local": "https://nystudio107.dev/",
        "critical": "https://nystudio107.com/"
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "scssName": "style.scss",
        "cssName": "style.css"
    },
    "globs": {
        "distCss": [
            "./build/css/*.css"
        ],
        "img": [
            "./web/img/"
        ],
        "components": [],
        "fonts": [
            "./build/fonts/fontello/font/*.{eot,ttf,woff,woff2}",
            "./src/fonts/*.{eot,ttf,woff,woff2}"
        ],
        "purgecss": [
            "./templates/**/*.{html,twig}"
        ],
        "purgecssWhitelist": [],
        "critical": [
            {
                "url": "",
                "template": "index"
            },
            {
                "url": "offline",
                "template": "offline"
            },
            {
                "url": "error",
                "template": "error"
            },
            {
                "url": "503",
                "template": "503"
            },
            {
                "url": "404",
                "template": "404"
            }
        ],
        "criticalWhitelist": [],
        "download": [
            {
                "url": "https://www.google-analytics.com/analytics.js",
                "dest": "./web/js/"
            }
        ],
        "distJs": [
            "./build/js/*.js",
            "./node_modules/axios/dist/axios.min.js",
            "./node_modules/lazysizes/lazysizes.min.js",
            "./node_modules/lazysizes/plugins/bgset/ls.bgset.min.js",
            "./node_modules/lazysizes/plugins/respimg/ls.respimg.min.js",
            "./node_modules/lazysizes/plugins/attrchange/ls.attrchange.min.js",
            "./node_modules/picturefill/dist/picturefill.min.js",
            "./node_modules/choices.js/assets/scripts/dist/choices.min.js",
            "./node_modules/vue/dist/vue.min.js",
            "./node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js",
            "./node_modules/video.js/dist/video.min.js",
            "./node_modules/cq-prolyfill/cq-prolyfill.min.js",
            "./node_modules/siema/dist/siema.min.js",
            "./node_modules/stickybits/dist/stickybits.min.js"
        ],
        "babelJs": [
            "./src/js/*.js",
            "./src/js/*.es6"
        ],
        "inlineCss": [
            "./node_modules/cookieconsent/build/cookieconsent.min.css"
        ],
        "inlineJs": [
            "./node_modules/fg-loadcss/src/loadCSS.js",
            "./node_modules/fg-loadcss/src/cssrelpreload.js",
            "./node_modules/fontfaceobserver/fontfaceobserver.js",
            "./node_modules/loadjs/dist/loadjs.min.js",
            "./node_modules/tiny-cookie/tiny-cookie.min.js",
            "./node_modules/cookieconsent/build/cookieconsent.min.js",
            "./src/js/tab-handler.js",
            "./src/js/register-service-worker.js",
            "./src/js/asyncload-site-fonts.js"
        ],
        "siteIcon": "./web/img/site/favicon.*"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "choices.js": "^3.0.3",
        "cookieconsent": "^3.0.6",
        "cq-prolyfill": "^0.4.0",
        "css-class-generator": "^1.0.1",
        "fg-loadcss": "^1.2.0",
        "fontfaceobserver": "^2.0.5",
        "lazysizes": "^2.0.6",
        "loadjs": "^3.3.1",
        "normalize.css": "^5.0.0",
        "perfect-scrollbar": "^1.3.0",
        "picturefill": "^3.0.2",
        "sag-bourbon-neat": "^2.1.1",
        "siema": "^1.5.1",
        "stickybits": "^3.2.0",
        "tiny-cookie": "^1.0.1",
        "v3-utility-library": "googlemaps/v3-utility-library",
        "video.js": "^6.6.3",
        "vue": "^2.5.16"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.49",
        "@babel/core": "^7.0.0-beta.49",
        "@babel/polyfill": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.49",
        "bourbon": "^5.0.0",
        "chalk": "^1.1.3",
        "critical": "^1.1.0",
        "css-mqpacker": "^6.0.2",
        "fancy-log": "^1.2.0",
        "git-rev-sync": "^1.7.1",
        "gulp": "^3.9.0",
        "gulp-autoprefixer": "^3.1.0",
        "gulp-babel": "^8.0.0-beta.2",
        "gulp-cached": "^1.1.1",
        "gulp-clean": "^0.4.0",
        "gulp-concat": "^2.6.0",
        "gulp-csscomb": "Pennebaker/gulp-csscomb#01eee9b",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^2.1.2",
        "gulp-download": "^0.0.1",
        "gulp-favicons": "^2.2.6",
        "gulp-filter": "^5.0.1",
        "gulp-fontello": "^0.4.6",
        "gulp-header": "^1.8.7",
        "gulp-if": "^2.0.1",
        "gulp-imagemin": "^4.1.0",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.3.0",
        "gulp-newer": "^1.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-postcss": "^7.0.1",
        "gulp-print": "^2.0.1",
        "gulp-purgecss": "^0.15.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.6.1",
        "gulp-rev": "^7.1.0",
        "gulp-sass": "^3.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.2.1",
        "gulp-streamify": "1.0.2",
        "gulp-tap": "^1.0.1",
        "gulp-uglify": "^1.5.4",
        "moment": "^2.14.1",
        "normalize-sass": "^1.0.0",
        "pa11y": "^4.11.0",
        "postcss-preset-env": "^3.2.2",
        "postcss-short": "^4.1.0",
        "sort-css-media-queries": "^1.3.4",
        "through2": "^2.0.3",
        "vinyl-source-stream": "^1.1.0"
    },
    "scripts": {
        "start": "gulp",
        "build": "gulp build"
    }
}
